<templateSet group="PHP">
  <template name="dd" value="die_dump($END$);" description="die_dump" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="er" value="error_log($END$);" description="error_log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ddd" value="echo '&lt;pre&gt;';var_dump($END$);exit;" description="var_dump then exit" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="eee" value="esc_attr_e( '$END$', 'txtd' );" description="echo esc_attr __" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="eesc" value="&lt;?php echo esc_attr($END$); ?&gt;" description="echo esc_attr" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="ehe" value="&lt;?php esc_html_e( '$END$', 'txtd' ); ?&gt;" description="esc_html_e" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="switch" value="switch( $END$ ) {&#10;    case 'something':&#10;        break;&#10;    default:&#10;        break;&#10;}" description="switch format" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>